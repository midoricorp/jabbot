{
    "bindings":[
        {   "name":"XMPP",
            "className":"org.wanna.jabbot.binding.xmpp.XmppBinding"
        },
        {
            "name":"IRC",
            "className":"org.wanna.jabbot.binding.irc.IrcBinding"
        },
        {
            "name":"CLI",
            "className":"org.wanna.jabbot.binding.cli.CliBinding"
        }
    ],
    "serverList":[
        {   "type" : "XMPP",
            "url":"localhost",
            "serverName":"localhost",
            "port":5222,
            "username":"jabbot",
            "password":"password",
            "identifier":"jabbot",
            "commandPrefix":"#",
            "debug":false,
            "parameters":{
                "allow_self_signed":true,
                "ping_interval":600
            },
            "rooms":[
                {"name":"jabbot-dev@conference.localhost","nickname":"jabbot"}
            ],
            "extensions":[
                {"name":"help","type":"command","className":"org.wanna.jabbot.command.HelpCommand"},
                {"name":"uptime","type":"command","className":"org.wanna.jabbot.command.UptimeCommand"},
                {"name":"quote","type":"command","className":"org.wanna.jabbot.extensions.quote.QuoteCommand"},
                {"name":"dice","type":"command","className":"org.wanna.jabbot.extensions.dice.RollCommand"},
                {"name":"chuck","type":"command","className":"org.wanna.jabbot.extensions.icndb.ChuckCommand"},
                {"name":"call","type":"command","className":"org.wanna.jabbot.extensions.call.CallCommand",
                    "configuration":{
                        "url":"http://voxcall.me",
                        "missing_user":"Hey! Who should I call? (flastname)"
                    }
                },
                {
                    "name": "jira",
                    "type":"command",
                    "className": "org.wanna.jabbot.extensions.jira.IssueViewer",
                    "configuration": {
                        "url": "https://jira.localhost",
                        "username": "username",
                        "password": "password"
                    }
                },
                {"name":"propaganda","type":"command","className":"org.wanna.jabbot.extensions.propaganda.PropagandaCommand"},
                {"name":"script","type":"command","className":"org.wanna.jabbot.extensions.script.ScriptCommand"}
            ]
        },
        {   "type" : "IRC",
            "url":"irc.hostname.com",
            "serverName":"hostname.com",
            "port":6667,
            "username":"Jabbot",
            "password":"password",
            "identifier":"Jabbot",
            "commandPrefix":"!",
            "rooms":[
                {"name":"jabbot-dev","nickname":"Jabbot"}
            ],
            "extensions":[
                {"name":"help","type":"command","className":"org.wanna.jabbot.command.HelpCommand"},
                {"name":"dice","type":"command","className":"org.wanna.jabbot.extensions.dice.RollCommand"},
                {"name":"attack","type":"command","className":"org.wanna.jabbot.extensions.foaas.AttackCommand"},
                {"name":"chuck","type":"command","className":"org.wanna.jabbot.extensions.icndb.ChuckCommand"},
                {"name":"translate","type":"command","className":"org.wanna.jabbot.extensions.translate.TranslateCommand"},
                {"name":"uptime","type":"command","className":"org.wanna.jabbot.command.UptimeCommand"}
            ]

        },
        {   "type" : "CLI",
            "url":"localhost",
            "identifier":"Jabbot",
            "commandPrefix":"#",
            "rooms":[
                {"name":"jabbot-dev","nickname":"Jabbot"}
            ],
            "extensions":[
                {"name":"help","type":"command","className":"org.wanna.jabbot.command.HelpCommand"},
                {"name":"dice","type":"command","className":"org.wanna.jabbot.extensions.dice.RollCommand"},
                {"name":"chuck","type":"command","className":"org.wanna.jabbot.extensions.icndb.ChuckCommand"},
                {"name":"uptime","type":"command","className":"org.wanna.jabbot.command.UptimeCommand"},
                {"name":"script","type":"command","className":"org.wanna.jabbot.extensions.script.ScriptCommand"}
            ]
        }
    ]
}
