{
    "bindings":[
        {   "name":"XMPP",
            "className":"org.wanna.jabbot.binding.xmpp.XmppBinding"
        },
        {
            "name":"IRC",
            "className":"org.wanna.jabbot.binding.irc.IrcBinding"
        },
        {
            "name":"CLI",
            "className":"org.wanna.jabbot.binding.cli.CliBinding"
        },
        {
            "className" : "org.wanna.jabbot.binding.spark.SparkBinding",
            "name" : "SPARK"
        },
        {
            "name":"SLACK",
            "className":"org.wanna.jabbot.binding.slack.SlackBinding"
        }
    ],
    "serverList":[
        {   "type" : "XMPP",
            "url":"localhost",
            "serverName":"localhost",
            "port":5222,
            "username":"jabbot",
            "password":"password",
            "identifier":"jabbot",
            "commandPrefix":"#",
            "debug":false,
            "parameters":{
                "allow_self_signed":true,
                "ping_interval":600
            },
            "rooms":[
                {"name":"jabbot-dev@conference.localhost","nickname":"jabbot"}
            ],
            "extensions":[
                {"name":"help","type":"command","className":"org.wanna.jabbot.command.HelpCommand"},
                {"name":"uptime","type":"command","className":"org.wanna.jabbot.command.UptimeCommand"},
                {"name": "script","type":"command","className": "org.wanna.jabbot.extensions.script.ScriptCommand",
                    "configuration": {
                        "loop_limit": "100",
                        "buffer_limit": "1000"
                    }
                }
            ]
        }
    ]
}
